#include <stdio.h>
main()
{
    int i, num, counter, meow=0;
    for(num=2; meow<=100; num++)
    {
        counter=0;
        for(i=2; i<num; i++)
        {
            if(num%i==0)
             {
                 counter++;
             }
        }
        if(counter==0)
        {
            meow++;
            printf("%d ", num);
        }
    }
}
